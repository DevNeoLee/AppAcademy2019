    <style>
      body {
        background-color: #FeFeFe;
      }
      .box-container {
        text-align: center;
      }

      .box buttons {


      }


      .box {
      display: inline-block;
      background-color: black; 
      border-radious: 2.5px;
      border-color: #E3E3E3; 
      border-style: solid;
      border-width: 1px;
      width: 600px;
      text-align: left;
      margin-bottom: 30px;
      }

    

      .box .padding {
        padding-left: 1.5em;
        padding-right: 1.5em;
        margin-bottom: 1.5em;
      }

      .box .image {
        margin-top: 4em;
        width: 120%;
        margin-bottom: 1em;
      }
      .box .hit {
        font-weight: bold;
        margin-bottom: 0.5em;

      }
    </style>
  
    </head>

  <body>
    <%= yield %>
  </body>
</html>
